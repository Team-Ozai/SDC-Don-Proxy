{
  "name": "proxy",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "install:navbar": "cd 'navbar' && npm install",
    "install:oh-service": "cd 'oh-service' && npm install",
    "install:tab-bar": "cd 'tab-bar' && npm install",
    "install:pledge-rewards": "cd 'pledge-rewards' && npm install",
    "install:va-service": "cd 'va-service' && npm install",
    "install:wwl-service": "cd 'wwl---service' && npm install",
    "install:proxy-server": "cd 'proxy-server' && npm install",
    "postinstall": "npm-run-all --sequential install:*",
    "start:navbar": "cd 'navbar' && npm run start",
    "start:oh-service": "cd 'oh-service' && npm run start",
    "start:tab-bar": "cd 'tab-bar' && npm run start",
    "start:pledge-rewards": "cd 'pledge-rewards' && npm run start",
    "start:va-service": "cd 'va-service' && npm run start",
    "start:wwl-service": "cd 'wwl---service' && npm run start",
    "start:proxy-server": "cd 'proxy-server' && npm run start",
    "start:all": "stmux -M -- [ \"npm run start:navbar\" : \"npm run start:oh-service\" : \"npm run start:tab-bar\" : \"npm run start:pledge-rewards\" : \"npm run start:va-service\" : \"npm run start:wwl-service\" : \"npm run start:proxy-server\" ]",
    "seed:oh-service": "cd 'oh-service' && npm run seed:db",
    "seed:pledge-rewards": "cd 'pledge-rewards' && npm run seed:database",
    "seed:va-service": "cd 'va-service' && npm run seed",
    "seed:wwl-service": "cd 'wwl---service' && npm run db:setup",
    "seed:all": "stmux -M -- [ \"npm run seed:oh-service\" : \"npm run seed:pledge-rewards\" : \"npm run seed:va-service\" : \"npm run seed:wwl-service\" ]",
    "build:navbar": "cd navbar/client && webpack -p --mode production",
    "build:oh-service": "cd oh-service && webpack -p --mode production",
    "build:tab-bar": "cd tab-bar/client && webpack -p --mode production",
    "build:pledge-rewards": "cd pledge-rewards/client && webpack -p --mode production",
    "build:va-service": "cd VA-Service && webpack -p --mode production",
    "build:wwl-service": "cd WWL---Service && webpack -p --mode production",
    "build:all": "stmux -M -- [ \"npm run build:navbar\" : \"npm run build:oh-service\" : \"npm run build:tab-bar\" : \"npm run build:pledge-rewards\" : \"npm run build:va-service\" : \"npm run build:wwl-service\" ]"
  },
  "private": true,
  "workspaces": [
    "navbar",
    "oh-service",
    "tab-bar",
    "rewards",
    "proxy-server",
    "VA-service",
    "WWL---Service"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/team-iroh/za-proxy.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/team-iroh/za-proxy/issues"
  },
  "homepage": "https://github.com/team-iroh/za-proxy#readme",
  "devDependencies": {
    "eslint": "^7.3.1",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "stmux": "^1.8.0",
    "ttab": "^0.6.1",
    "webfontloader": "^1.6.28",
    "webpack": "^4.43.0"
  }
}
