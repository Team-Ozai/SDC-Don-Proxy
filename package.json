{
  "name": "proxy",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "install:navbar": "yarn --cwd 'navbar' install",
    "install:oh-service": "yarn --cwd 'oh-service' install",
    "install:pledge-rewards": "yarn --cwd 'pledge-rewards' install",
    "install:va-service": "yarn --cwd 'va-service' install",
    "install:wwl-service": "yarn --cwd 'wwl---service' install",
    "install:proxy-server": "yarn --cwd 'proxy-server' install",
    "install:all": "npm-run-all --parallel install:*",

    "start:navbar": "yarn --cwd 'navbar' start",
    "start:oh-service": "yarn --cwd 'oh-service' start",
    "start:pledge-rewards": "yarn --cwd 'pledge-rewards' start",
    "start:va-service": "yarn --cwd 'va-service' start",
    "start:wwl-service": "yarn --cwd 'wwl---service' start",
    "start:proxy-server": "yarn --cwd 'proxy-server' start",
    "start:all": "npm-run-all --sequential start:*",

    "seed:oh-service": "yarn --cwd 'oh-service' seed:db",
    "seed:pledge-rewards": "yarn --cwd 'pledge-rewards' seed:database",
    "seed:va-service": "yarn --cwd 'va-service' seed",
    "seed:wwl-service": "yarn --cwd 'wwl---service' db:setup",
    "seed:all": "stmux -M -- [ \"yarn seed:oh-service\" : \"yarn seed:pledge-rewards\" : \"yarn seed:va-service\" : \"yarn seed:wwl-service\" ]",


    "build:navbar": "",
    "build:oh-service": "",
    "build:pledge-rewards": "",
    "build:va-service": "",
    "build:wwl-service": "",
    "build:proxy-server": "",


    "dev": "stmux -M -- [ \"yarn start:navbar\" : \"yarn start:oh-service\" : \"yarn start:pledge-rewards\" : \"yarn start:va-service\" : \"yarn start:wwl-service\" : \"yarn start:proxy-server\"]"
  },
  "private": true,
  "workspaces": [
    "navbar",
    "oh-serice",
    "pledge-rewards",
    "proxy-server",
    "VA-service",
    "WWL---Service"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/team-iroh/za-proxy.git"
  },
